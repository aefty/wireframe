{
  "GET": {
    "/api/stats": {
      "sync": [
        "app.getData",
        "app.doStats"
      ],
      "async": [
        "app.writeDB",
        "app.getData"
      ]
    },
    "*": {
      "sync": [
        "error.wrongURL"
      ]
    }
  },
  "POST": {
    "/api/login": {
      "sync": [
        "app.login",
        "app.getUser"
      ]
    },
    "*": {
      "async": [
        "error.wrongURL"
      ]
    }
  },
  "DELETE": {
    "*": {
      "async": [
        "error.wrongMethod"
      ]
    }
  }
}
